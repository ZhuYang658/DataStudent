  <!--configuration这是配置文件的根节点，方便改其中的参数-->
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    <!--这是运行环境-->
  </startup>
  <!--给配置文件加可改参数-->
  <!--数据库我们有可能改变，所以在这里方便后期改变，用标签connectionStrings-->
  <connectionStrings>
    <!--一点要给添加加上名字，原因是如果有多个防止出错-->
    <add name="Constr" connectionString="Server=USER-20190129ES;Database=SMDB;UID=sa;PWD=123" />
    <!--添加访问数据库的字符串-->
  </connectionStrings>
  <!--版本-->
  <appSettings>
    <add key="version" value="v1.0.0" />
    <add key="webadd" value="http://www.yltedu.com"/>
  </appSettings>
 
  
  
